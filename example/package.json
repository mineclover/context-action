{
  "name": "example",
  "version": "1.0.0",
  "private": true,
  "description": "Example application for @context-action/core library",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "echo 'No tests for example app'",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "format": "biome format src",
    "format:write": "biome format --write src",
    "check": "biome check src",
    "check:fix": "biome check --write src",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@context-action/core": "workspace:*",
    "@context-action/jotai": "workspace:*",
    "@context-action/logger": "workspace:*",
    "@context-action/react": "workspace:*",
    "dotenv": "^16.4.5",
    "jotai": "^2.10.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@tailwindcss/typography": "0.5.16",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "postcss": "8.5.6",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.11",
    "typescript": "^5.7.2",
    "vite": "^6.0.6"
  }
}